{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "js/**/*.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/register", "dest": "/api/register.js" },
    { "src": "/api/query/(.*)", "dest": "/api/query/[extension].js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}